{
  "expo": {
    "name": "Witcher's Satchel",
    "slug": "witcherssatchel",
    "scheme": "witcherssatchel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.witcherssatchel",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We need your location to show local weather data.",
        "NSLocationAlwaysUsageDescription": "We need your location to show local weather data."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.witcherssatchel",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["assets/StoryScript-Regular.ttf"]
        }
      ],
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "potion": {
              "foregroundImage": "./assets/potion-sf.png",
              "backgroundColor": "#cc0000"
            }
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Witcher's Satchel to access your location to fetch local weather information."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "abae28c7-2382-43d8-b588-68ec1c5f71e0"
      }
    },
    "owner": "404pandas"
  }
}
